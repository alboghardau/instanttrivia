<?xml version="1.0" encoding="utf-8"?>
<inset
    android:inset="3dp"
    xmlns:android="http://schemas.android.com/apk/res/android">
<ripple
    android:color="@color/light_blue_600"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/com_facebook_blue"/>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp"
                />
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/button_lollipop_notpressed_fb">
    </item>
</ripple>
</inset>